{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "semi": true,
  "printWidth": 120,
  "plugins": ["prettier-plugin-organize-imports"]
}